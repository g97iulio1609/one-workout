{
  "id": "workout-generation",
  "version": "2.0.0",
  "type": "agent",
  "description": "Multi-agent workout program generator using SDK v4.0 with durable execution",
  "interface": {
    "input": { "$ref": "./schema.ts#WorkoutGenerationInputSchema" },
    "output": { "$ref": "./schema.ts#WorkoutGenerationOutputSchema" }
  },
  "mcpServers": {},
  "config": {
    "tier": "balanced",
    "temperature": 1,
    "maxSteps": 50,
    "maxTokens": 8192,
    "timeout": 600000,
    "executionMode": "durable",
    "skipSynthesis": true,
    "outputArtifact": "finalProgram",
    "durability": {
      "enabled": true,
      "maxDurationMs": 1800000,
      "retry": {
        "maxAttempts": 5,
        "backoffMs": 2000,
        "backoffMultiplier": 2
      },
      "checkpointStrategy": "step"
    }
  }
}
