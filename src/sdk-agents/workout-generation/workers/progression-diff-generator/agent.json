{
  "id": "progression-diff-generator",
  "version": "2.0.0",
  "type": "agent",
  "description": "Generates progression diffs for weeks 2-4 based on Week 1 template",
  "interface": {
    "input": { "$ref": "progression-diff-generator:input" },
    "output": { "$ref": "progression-diff-generator:output" }
  },
  "mcpServers": {},
  "config": {
    "tier": "balanced",
    "temperature": 1,
    "maxSteps": 10,
    "maxTokens": 8192,
    "timeout": 120000,
    "executionMode": "durable",
    "durability": {
      "enabled": true,
      "maxDurationMs": 600000,
      "retry": {
        "maxAttempts": 3,
        "backoffMs": 1000,
        "backoffMultiplier": 2
      },
      "checkpointStrategy": "step"
    }
  }
}
